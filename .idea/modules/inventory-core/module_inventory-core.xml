<?xml version="1.0" encoding="UTF-8"?>
<project name="module_inventory-core" default="compile.module.inventory-core">
  <dirname property="module.inventory-core.basedir" file="${ant.file.module_inventory-core}"/>
  
  <property name="module.jdk.home.inventory-core" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.inventory-core" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.inventory-core" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.inventory-core" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="inventory-core.output.dir" value="undefined"/>
  <property name="inventory-core.testoutput.dir" value="undefined"/>
  
  <path id="inventory-core.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="inventory-core.module.production.classpath">
    <path refid="${module.jdk.classpath.inventory-core}"/>
  </path>
  
  <path id="inventory-core.runtime.production.module.classpath"/>
  
  <path id="inventory-core.module.classpath">
    <path refid="${module.jdk.classpath.inventory-core}"/>
  </path>
  
  <path id="inventory-core.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.inventory-core">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.inventory-core">
    <patternset refid="excluded.from.module.inventory-core"/>
  </patternset>
  
  
  <target name="compile.module.inventory-core" depends="compile.module.inventory-core.production,compile.module.inventory-core.tests" description="Compile module inventory-core"/>
  
  <target name="compile.module.inventory-core.production" depends="register.custom.compilers" description="Compile module inventory-core; production classes"/>
  
  <target name="compile.module.inventory-core.tests" depends="register.custom.compilers,compile.module.inventory-core.production" description="compile module inventory-core; test classes" unless="skip.tests"/>
  
  <target name="clean.module.inventory-core" description="cleanup module">
    <delete dir="${inventory-core.output.dir}"/>
    <delete dir="${inventory-core.testoutput.dir}"/>
  </target>
</project>