<?xml version="1.0" encoding="UTF-8"?>
<project name="module_spring-skeleton" default="compile.module.spring-skeleton">
  <dirname property="module.spring-skeleton.basedir" file="${ant.file.module_spring-skeleton}"/>
  
  <property name="module.jdk.home.spring-skeleton" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.spring-skeleton" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.spring-skeleton" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.spring-skeleton" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="spring-skeleton.output.dir" value="undefined"/>
  <property name="spring-skeleton.testoutput.dir" value="undefined"/>
  
  <path id="spring-skeleton.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="spring-skeleton.module.production.classpath">
    <path refid="${module.jdk.classpath.spring-skeleton}"/>
  </path>
  
  <path id="spring-skeleton.runtime.production.module.classpath"/>
  
  <path id="spring-skeleton.module.classpath">
    <path refid="${module.jdk.classpath.spring-skeleton}"/>
  </path>
  
  <path id="spring-skeleton.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.spring-skeleton">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.spring-skeleton">
    <patternset refid="excluded.from.module.spring-skeleton"/>
  </patternset>
  
  
  <target name="compile.module.spring-skeleton" depends="compile.module.spring-skeleton.production,compile.module.spring-skeleton.tests" description="Compile module spring-skeleton"/>
  
  <target name="compile.module.spring-skeleton.production" depends="register.custom.compilers" description="Compile module spring-skeleton; production classes"/>
  
  <target name="compile.module.spring-skeleton.tests" depends="register.custom.compilers,compile.module.spring-skeleton.production" description="compile module spring-skeleton; test classes" unless="skip.tests"/>
  
  <target name="clean.module.spring-skeleton" description="cleanup module">
    <delete dir="${spring-skeleton.output.dir}"/>
    <delete dir="${spring-skeleton.testoutput.dir}"/>
  </target>
</project>